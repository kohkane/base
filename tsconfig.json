{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "lib": [
      "es5"
    ],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@models/*": ["./src/models/*"],
      "@functions/*": ["./src/functions/*"],
      "@schemas/*": ["./src/schemas/*"]
    }
  },
  "include": [
    "./src/functions/**/*",
    "./src/models/**/*",
  ]
}
